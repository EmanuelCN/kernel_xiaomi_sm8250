&tlmm {

/* AW8697_RESET_N */
        awinic_gpio_reset: awinic_gpio_reset {
                mux {
                         pins = "gpio130";
                         function = "gpio";
                };

                config {
                        pins = "gpio130";
                        drive-strength = <2>;
                        bias-disable;
                        output-low;
                };
        };
        /* AW8697_RESET_N, state device active*/
        awinic_gpio_reset_output_high: awinic_gpio_reset_output_high {
                mux {
                        pins = "gpio130";
                        function = "gpio";
                };

                config {
                        pins = "gpio130";
                        drive-strength = <2>;
                        bias-disable;
                        output-high;
                };
        };

        /* AW8697_INTERRUPT_N */
        awinic_gpio_int: awinic_gpio_int {
                mux {
                        pins = "gpio129";
                        function = "gpio";
                };

                config {
                        pins = "gpio129";
                        drive-strength = <2>;
                        bias-pull-up;
                        input-enable;
                };
        };
};

/* fingerprint start */
&L13A {
	regulator-min-microvolt = <3000000>;
	regulator-max-microvolt = <3000000>;
	qcom,init-voltage = <3000000>;
};
&L5C {
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <1800000>;
	qcom,init-voltage = <1800000>;
};
/* fingerprint end */

&tlmm {
		/* FP_RESET_N */
	msm_gpio_reset: msm_gpio_reset {
		mux {
			pins = "gpio24";
			function = "gpio";
		};
		config {
			pins = "gpio24";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
		/* FP_RESET_N, state device active*/
	msm_gpio_reset_output_high: msm_gpio_reset_output_high {
		mux {
			pins = "gpio24";
			function = "gpio";
		};
		config {
			pins = "gpio24";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};
		/* FP_INT_N */
	msm_gpio_irq: msm_gpio_irq {
		mux {
			pins = "gpio23";
			function = "gpio";
		};
		config {
			pins = "gpio23";
			drive-strength = <2>;
			bias-pull-down;
		};
	};
};


